        </activity>
        <activity android:name=".CheckMailActivity"></activity>
        <activity android:name=".LogInActivity"></activity>
        <activity android:name=".NicknameActivity"></activity>
        <activity android:name=".RegisterProfileActivity"></activity>
        <activity android:name=".HomePageActivity"></activity>
        <activity android:name=".PostOneActivity"></activity>
        <activity android:name=".PostTwoActivity"></activity>
        <activity android:name=".SendPostActivity"></activity>
        <activity android:name=".JoinPostActivity"></activity>
    </application>



</manifest>